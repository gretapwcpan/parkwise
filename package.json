{
  "name": "parking-space-prototype",
  "version": "1.0.0",
  "description": "A cross-platform parking space booking system with real-time navigation",
  "private": true,
  "scripts": {
    "install:all": "npm run install:backend && npm run install:frontend",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm start",
    "dev:frontend": "cd frontend && npm start",
    "start": "npm run dev",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "clean": "npm run clean:backend && npm run clean:frontend",
    "clean:backend": "cd backend && rm -rf node_modules package-lock.json",
    "clean:frontend": "cd frontend && rm -rf node_modules package-lock.json build",
    "test": "echo \"No tests configured yet\""
  },
  "workspaces": [
    "backend",
    "frontend"
  ],
  "devDependencies": {
    "concurrently": "^7.6.0",
    "turbo": "^1.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gretapwcpan/parking-space-prototype.git"
  },
  "keywords": [
    "parking",
    "booking",
    "real-time",
    "navigation",
    "react",
    "node",
    "express"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/gretapwcpan/parking-space-prototype/issues"
  },
  "homepage": "https://github.com/gretapwcpan/parking-space-prototype#readme"
}
